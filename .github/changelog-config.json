{
    "categories": [
        {
            "title": "## 🚀 Features",
            "labels": ["feat", "enhancement"]
        },
        {
            "title": "## 🐛 Fixes",
            "labels": ["fix", "bug"]
        },
        {
            "title": "## 🧪 Tests",
            "labels": ["test"]
        },
        {
            "title": "## 📚 Documentation",
            "labels": ["docs"]
        },
        {
            "title": "## 🔧 Maintenance",
            "labels": ["chore", "dependencies", "refactor"]
        }
    ],
    "template": "${{CHANGELOG}}\n\n## 📦 Assets\nDownload the appropriate binary for your system from the assets below.\n\nFor checksums, download the corresponding `.sha256` file.",
    "pr_template": "- ${{TITLE}} (#${{NUMBER}})",
    "empty_template": "- No changes in this release",
    "max_tags_to_fetch": 200,
    "max_pull_requests": 200,
    "max_back_track_time_days": 365
}
